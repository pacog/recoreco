{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
        "React": true
    },

    "ecmaFeatures": {
        "jsx": true
    },

    "plugins": [
        "react"
    ],
    "rules": {
      "no-undef": "error",
      "no-unused-vars": "error",
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
    }

}
